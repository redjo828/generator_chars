import string

def generate_special_bytes(length):
    count = 0
    while count < length:
        for upper in string.ascii_uppercase:
            for lower in string.ascii_lowercase:
                for num in range(10):
                    yield upper + lower + str(num)
                    count += 1
                    if count >= length:
                        return

length = int(input("Input length of chars: ")) #0 - 50000
if length:
    gen = generate_special_bytes(length)
    result = ''.join([next(gen) for _ in range(length)])
    result = result[:length]
    print("\nResult:", result)
    print("\nLength:", len(result))
else:
    print("Invalid length entered.")

